{
    "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
    "name": "magnum",
    "version": "1.0.0",
    "description": "This file contains details of the assets used template by magnum.",
    "license": "GPL-2.0-or-later",
    "assets": [
        {
            "name": "template.bootstrap.style",
            "type": "style",
            "uri": "bootstrap.min.css"
        },
        {
            "name": "template.styles",
            "type": "style",
            "uri": "template.css"
        },
        {
            "name": "template.bootstrap.script",
            "type": "script",
            "uri": "bootstrap.bundle.min.js",
            "attributes": {
                "defer": true
            }
        },
        {
            "name": "template.jquery",
            "type": "script",
            "uri": "jquery-3.7.1.min.js",
            "attributes": {
                "defer": true
            }
        },
        {
            "name": "template.lightbox",
            "type": "script",
            "uri": "lightbox.min.js",
            "attributes": {
                "defer": true
            }
        },
        {
            "name": "template.prism",
            "type": "script",
            "uri": "prism.js",
            "attributes": {
                "defer": true
            }
        },
        {
            "name": "template.go_top",
            "type": "script",
            "uri": "go_top.js",
            "attributes": {
            "defer": true
            }
        }
    ]
}
